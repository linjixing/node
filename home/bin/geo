#!/usr/bin/env sh

curl -R -H 'Cache-Control: no-cache' -Lo /home/bin/geoip.dat https://github.com/Loyalsoldier/v2ray-rules-dat/releases/latest/download/geoip.dat
curl -R -H 'Cache-Control: no-cache' -Lo /home/bin/geosite.dat https://github.com/Loyalsoldier/v2ray-rules-dat/releases/latest/download/geosite.dat

exec "$@"
